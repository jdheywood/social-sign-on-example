{
  "name": "social-sign-on-example",
  "version": "0.1.0",
  "description": "A simple implementation of social sign on for securing an Express application",
  "repository": {
    "type": "git",
    "url": "http://github.com/jdheywood/social-sign-on-example.git"
  },
  "main": "src/app.js",
  "scripts": {
    "lint": "jshint --verbose src/.",
    "start": "nodemon src/*.js --exec babel-node",
    "build": "babel src -d dist",
    "serve": "node dist/app.js",
    "test": "mocha --recursive --compilers js:babel-register --reporter spec",
    "test:coverage": "./node_modules/.bin/istanbul cover _mocha test/**.js -- --require babel-register",
    "generate-docs": "./node_modules/.bin/jsdoc src -r",
    "watch": "nodemon -e css,js,pug -w src -w views --ignore 'dist' -x 'npm run start'",
    "env": "env"
  },
  "author": "James Heywood <hello@jdheywood.com>",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-istanbul": "^0.12.1",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.22.0",
    "chai": "^3.5.0",
    "istanbul": "^1.1.0-alpha.1",
    "jsdoc": "^3.4.3",
    "jshint": "^2.9.4",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0"
  },
  "dependencies": {
    "body-parser": "^1.16.0",
    "cloudinary": "^1.7.0",
    "connect-ensure-login": "^0.1.1",
    "connect-flash": "^0.1.1",
    "errorhandler": "^1.5.0",
    "express": "^4.14.1",
    "express-fileupload": "^0.1.1",
    "express-session": "^1.15.0",
    "generate-url": "^1.1.0",
    "method-override": "^2.3.7",
    "moment": "^2.17.1",
    "moment-timezone": "^0.5.11",
    "mongoose": "^4.8.1",
    "morgan": "^1.7.0",
    "node-uuid": "^1.4.7",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^4.0.0",
    "pug": "^2.0.0-beta11",
    "q": "^1.4.1",
    "sendgrid": "^4.7.1",
    "serve-static": "^1.11.2",
    "slack-node": "^0.1.8",
    "underscore": "^1.8.3",
    "validator": "^6.2.1"
  }
}
